<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Basic -->
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />

    <!-- Mobile Metas -->
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />

    <!-- Site Metas -->
    <title><%- title || "Default Title" %></title>
    <meta
      name="keywords"
      content="Tech tutorials, Digital trends, Software updates, Gadgets reviews, Tech news"
    />
    <meta name="description" content="<%- description %>" />

    <!-- Site Icons -->
    <link rel="shortcut icon" href="<%- icon %>" type="image/x-icon" />
    <link rel="apple-touch-icon" href="<%- icon %>" />

    <!-- Design fonts -->
    <link
      href="https://fonts.googleapis.com/css?family=Ubuntu:300,400,400i,500,700"
      rel="stylesheet"
    />
    <link
      href="https://fonts.googleapis.com/css?family=Roboto:300,400,400i,500,700,900"
      rel="stylesheet"
    />
    <link rel="stylesheet" href="/admin/assets/vendor/fonts/boxicons.css" />

    <!-- Bootstrap core CSS -->
    <link href="/frontend/css/bootstrap.css" rel="stylesheet" />

    <!-- FontAwesome Icons core CSS -->
    <link href="/frontend/css/font-awesome.min.css" rel="stylesheet" />

    <!-- Custom styles for this template -->
    <link href="/frontend/style.css" rel="stylesheet" />

    <!-- Responsive styles for this template -->
    <link href="/frontend/css/responsive.css" rel="stylesheet" />

    <!-- Colors for this template -->
    <link href="/frontend/css/colors.css" rel="stylesheet" />
  </head>

  <body>
    <script
      defer
      data-domain="posttware.com"
      src="https://plausible.io/js/script.js"
    ></script>

    <!-- LOADER -->
    <div id="preloader">
      <img class="preloader" src="/frontend/images/loader.gif" alt="" />
    </div>
    <!-- end loader -->
    <!-- END LOADER -->

    <div id="wrapper">
      <div class="collapse top-search" id="collapseExample">
        <div class="card card-block">
          <div class="newsletter-widget text-center">
            <form class="form-inline" method="get" action="/category/?id=0205">
              <input
                type="text"
                name="search"
                class="form-control"
                placeholder="What you are looking for?"
              />
              <input
                type="hidden"
                name="id"
                value="0205"
                class="form-control"
                placeholder="What you are looking for?"
              />
              <input
                type="hidden"
                name="p"
                value="1"
                class="form-control"
                placeholder="What you are looking for?"
              />
              <button type="submit" class="btn btn-primary">
                <i class="fa fa-search"></i>
              </button>
            </form>
          </div>
          <!-- end newsletter -->
        </div>
      </div>
      <!-- end top-search -->

      <div class="topbar-section">
        <div class="container-fluid">
          <div class="row">
            <div class="col-lg-4 col-md-6 col-sm-6 hidden-xs-down">
              <!-- <div class="topsocial">
                            <a href="#" data-toggle="tooltip" data-placement="bottom" title="Facebook"><i class="fa fa-facebook"></i></a>
                            <a href="#" data-toggle="tooltip" data-placement="bottom" title="Youtube"><i class="fa fa-youtube"></i></a>
                            <a href="#" data-toggle="tooltip" data-placement="bottom" title="Pinterest"><i class="fa fa-pinterest"></i></a>
                            <a href="#" data-toggle="tooltip" data-placement="bottom" title="Twitter"><i class="fa fa-twitter"></i></a>
                            <a href="#" data-toggle="tooltip" data-placement="bottom" title="Flickr"><i class="fa fa-flickr"></i></a>
                            <a href="#" data-toggle="tooltip" data-placement="bottom" title="Instagram"><i class="fa fa-instagram"></i></a>
                            <a href="#" data-toggle="tooltip" data-placement="bottom" title="Google+"><i class="fa fa-google-plus"></i></a>
                        </div> -->
            </div>

            <div class="col-lg-4 hidden-md-down">
              <div class="topmenu text-center">
                <ul class="list-inline">
                  <li class="list-inline-item">
                    <a href="/category/?id=0203&p=1"
                      ><i class="fa fa-star"></i> Trend</a
                    >
                  </li>
                  <li class="list-inline-item">
                    <a href="/category/?id=0204&p=1"
                      ><i class="fa fa-bolt"></i> Recent</a
                    >
                  </li>
                  <li class="list-inline-item">
                    <a href="/contact"
                      ><i class="fa fa-user-circle-o"></i> Write for us</a
                    >
                  </li>
                </ul>
                <!-- end ul -->
              </div>
              <!-- end topmenu -->
            </div>
            <!-- end col -->

            <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
              <div class="topsearch text-right">
                <a
                  data-toggle="collapse"
                  href="#collapseExample"
                  aria-expanded="false"
                  aria-controls="collapseExample"
                  ><i class="fa fa-search"></i> Search</a
                >
              </div>
              <!-- end search -->
            </div>
            <!-- end col -->
          </div>
          <!-- end row -->
        </div>
        <!-- end header-logo -->
      </div>
      <!-- end topbar -->

      <div class="header-section">
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              <div class="logo">
                <a href="/"><img src="/frontend/images/logo.png" alt="" /></a>
              </div>
              <!-- end logo -->
            </div>
          </div>
          <!-- end row -->
        </div>
        <!-- end header-logo -->
      </div>
      <!-- end header -->

      <header class="header">
        <div class="container">
          <nav class="navbar navbar-inverse navbar-toggleable-md">
            <button
              class="navbar-toggler"
              type="button"
              data-toggle="collapse"
              data-target="#cloapediamenu"
              aria-controls="cloapediamenu"
              aria-expanded="false"
              aria-label="Toggle navigation"
            >
              <span class="navbar-toggler-icon"></span>
            </button>
            <div
              class="collapse navbar-collapse justify-content-md-center"
              id="cloapediamenu"
            >
              <ul class="navbar-nav">
                <li class="nav-item">
                  <a class="nav-link color-red-hover" href="/">Home</a>
                </li>
                <% result.forEach(element => { %>
                <li class="nav-item">
                  <a
                    class="nav-link color-green-hover"
                    href="/category/?id=<%- element.id %>&p=1"
                    ><%- element.name %></a
                  >
                </li>
                <% }) %>

                <!-- <li class="nav-item">
                                <a class="nav-link color-red-hover" href="/frontend/blog-category-02.html">Food</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link color-aqua-hover" href="/frontend/blog-category-03.html">Lifestyle</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link color-green-hover" href="/frontend/blog-category-04.html">Travel</a>
                            </li>  
                            <li class="nav-item">
                                <a class="nav-link color-yellow-hover" href="/frontend/blog-category-05.html"><i class="fa fa-play-circle-o"></i> Vlogs</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link color-grey-hover" href="/frontend/blog-category-06.html">Health</a>
                            </li> -->
              </ul>
            </div>
          </nav>
        </div>
        <!-- end container -->
      </header>
      <!-- end header -->

      <%- body %>

      <footer class="footer">
        <div class="container">
          <div class="row">
            <div class="col-lg-4 col-md-12 col-sm-12 col-xs-12">
              <div class="widget">
                <h2 class="widget-title">Recent Posts</h2>
                <div class="blog-list-widget">
                  <div class="list-group">
                    <% recent.forEach(element => { %>
                    <a
                      href="/blog/<%- element.id %>"
                      class="list-group-item list-group-item-action flex-column align-items-start"
                    >
                      <div class="w-100 justify-content-between">
                        <img
                          src="<%- element.mainImg %>"
                          alt=""
                          class="float-left"
                          style="
                            max-width: 130px !important;
                            height: auto;
                            object-fit: cover;
                          "
                        />
                        <h5 class="mb-1"><%- element.title %></h5>
                        <small
                          ><%- element.createdAt.toLocaleDateString('en-EN', {
                          day: 'numeric', month: 'long', year: 'numeric' });
                          %></small
                        >
                      </div>
                    </a>
                    <% }) %>
                  </div>
                </div>
                <!-- end blog-list -->
              </div>
              <!-- end widget -->
            </div>
            <!-- end col -->

            <div class="col-lg-4 col-md-12 col-sm-12 col-xs-12">
              <div class="widget">
                <h2 class="widget-title">Popular Posts</h2>
                <div class="blog-list-widget">
                  <div class="list-group">
                    <% if (popular[0]) { %>
                    <a
                      href="/blog/<%- popular[0].id %>"
                      class="list-group-item list-group-item-action flex-column align-items-start"
                    >
                      <div class="w-100 justify-content-between">
                        <img
                          src="<%- popular[0].mainImg %>"
                          alt=""
                          class="float-left"
                          style="
                            max-width: 130px !important;
                            height: auto;
                            object-fit: cover;
                          "
                        />
                        <h5 class="mb-1"><%- popular[0].title %></h5>
                        <small
                          ><i class="fa fa-eye"></i> <%- popular[0].click
                          %></small
                        >
                      </div>
                    </a>
                    <% } %> <% if (popular[1]) { %>
                    <a
                      href="/blog/<%- popular[1].id %>"
                      class="list-group-item list-group-item-action flex-column align-items-start"
                    >
                      <div class="w-100 justify-content-between">
                        <img
                          src="<%- popular[1].mainImg %>"
                          alt=""
                          class="float-left"
                          style="
                            max-width: 130px !important;
                            height: auto;
                            object-fit: cover;
                          "
                        />
                        <h5 class="mb-1"><%- popular[1].title %></h5>
                        <small
                          ><i class="fa fa-eye"></i> <%- popular[1].click
                          %></small
                        >
                      </div>
                    </a>
                    <% } %> <% if (popular[2]) { %>
                    <a
                      href="/blog/<%- popular[2].id %>"
                      class="list-group-item list-group-item-action flex-column align-items-start"
                    >
                      <div class="w-100 justify-content-between">
                        <img
                          src="<%- popular[2].mainImg %>"
                          alt=""
                          class="float-left"
                          style="
                            max-width: 130px !important;
                            height: auto;
                            object-fit: cover;
                          "
                        />
                        <h5 class="mb-1"><%- popular[2].title %></h5>
                        <small
                          ><i class="fa fa-eye"></i> <%- popular[2].click
                          %></small
                        >
                      </div>
                    </a>
                    <% } %>
                  </div>
                </div>
                <!-- end blog-list -->
              </div>
              <!-- end widget -->
            </div>
            <!-- end col -->

            <div class="col-lg-4 col-md-12 col-sm-12 col-xs-12">
              <div class="widget">
                <h2 class="widget-title">Popular Categories</h2>
                <div class="link-widget">
                  <ul>
                    <% result.forEach(element => { %> <% var i = 0; %>
                    <li>
                      <a href="/category/?id=<%- element.id %>&p=1"
                        ><%- element.name %>
                        <span>
                          <% allPost.forEach(count => { %> <%
                          if(count.categoryId == element.id) { %> <% i++ %> <%
                          }%> <% }) %> (<%- i %>)
                        </span>
                      </a>
                    </li>
                    <% }) %>
                  </ul>
                </div>
                <!-- end link-widget -->
              </div>
              <!-- end widget -->
            </div>
            <!-- end col -->
          </div>
          <!-- end row -->

          <hr class="invis1" />

          <div class="row">
            <div class="col-lg-8 offset-lg-2">
              <div class="widget">
                <div class="footer-text text-center">
                  <a href="/frontend/index.html"
                    ><img
                      src="/frontend/images/flogo.png"
                      alt=""
                      class="img-fluid"
                  /></a>
                  <p>
                    Welcome to Posttware! Your hub for tech trends, insightful
                    analyses, and digital discoveries. Stay updated, stay
                    inspired!
                  </p>
                  <div class="social">
                    <a
                      href="<%- footer.urls.facebook %>"
                      data-toggle="tooltip"
                      data-placement="bottom"
                      title="Facebook"
                      ><i class="fa fa-facebook"></i
                    ></a>
                    <a
                      href="<%- footer.urls.twitter %>"
                      data-toggle="tooltip"
                      data-placement="bottom"
                      title="Twitter"
                      ><i class="fa fa-twitter"></i
                    ></a>
                    <a
                      href="<%- footer.urls.instagram %>"
                      data-toggle="tooltip"
                      data-placement="bottom"
                      title="Instagram"
                      ><i class="fa fa-instagram"></i
                    ></a>
                  </div>

                  <hr class="invis" />
                </div>
                <!-- end footer-text -->
              </div>
              <!-- end widget -->
            </div>
            <!-- end col -->
          </div>
          <div class="row">
            <div class="col-md-12 text-center">
              <ul
                class="d-flex justify-content-center align-items-center footer-ul"
              >
                <li>
                  <a href=""
                    ><small class="text-muted">Conditions of Use</small></a
                  >
                </li>
                <li>
                  <a href=""
                    ><small class="text-muted">Privacy Notice</small></a
                  >
                </li>
                <li>
                  <a href=""
                    ><small class="text-muted"
                      >Your Ads Privacy Choices</small
                    ></a
                  >
                </li>
              </ul>
              <br />
              <div class="copyright">
                &copy; 2024 <a href="http://html.design">fivehub.com</a>.
              </div>
            </div>
          </div>
        </div>
        <!-- end container -->
      </footer>
      <!-- end footer -->

      <div class="dmtop">Scroll to Top</div>
    </div>
    <!-- end wrapper -->

    <!-- Core JavaScript
    ================================================== -->
    <script src="/frontend/js/jquery.min.js"></script>
    <script src="/frontend/js/tether.min.js"></script>
    <script src="/frontend/js/bootstrap.min.js"></script>
    <script src="/frontend/js/custom.js"></script>
